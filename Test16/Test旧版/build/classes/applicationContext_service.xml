<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns:aop="http://www.springframework.org/schema/aop"
	   xmlns:tx="http://www.springframework.org/schema/tx"
	   xsi:schemaLocation="
		http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
		http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
		http://www.springframework.org/schema/tx http://www.springframework.org/schema/tx/spring-tx-2.5.xsd">
<!-- 配置service -->
<bean id="questionService" class="service.QuestionService">
	<property name="questionDao" ref="questionDao"></property>
</bean>

<bean id="examService" class="service.ExamService">
	<property name="examDao" ref="examDao"></property>
</bean>

<bean id="studentService" class="service.StudentService">
	<property name="studentDao" ref="studentDao"></property>
</bean>

<bean id="paperService" class="service.PaperService">
	<property name="paperDao" ref="paperDao"></property>
</bean>

<bean id="roleService" class="service.RoleService">
	<property name="roleDao" ref="roleDao"></property>
</bean>

</beans>